pages = {2245--54},
pmid = {20566501},
title = {{The interpretation of habitat preference metrics under use-availability designs.}},
url = {http://www.pubmedcentral.nih.gov/articlerender.fcgi?artid=2894962{\&}tool=pmcentrez{\&}rendertype=abstract},
