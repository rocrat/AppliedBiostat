journal = {Food and chemical toxicology : an international journal published for the British Industrial Biological Research Association},
keywords = {Animals,Benchmarking,Benchmarking: methods,Benchmarking: standards,Environmental Exposure,Environmental Exposure: analysis,Food Contamination,Humans,Models, Statistical,Mycotoxins,Mycotoxins: analysis,Pesticides,Pesticides: analysis,Risk Assessment,Risk Assessment: methods},
month = {dec},
number = {12},
pages = {2906--25},
pmid = {19682530},
