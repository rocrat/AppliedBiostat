journal = {Bioinformatics (Oxford, England)},
keywords = {CoDa,LASSO},
mendeley-tags = {CoDa,LASSO},
month = {oct},
number = {19},
pages = {3172--80},
pmid = {26048598},
title = {{CCLasso: correlation inference for compositional data through Lasso.}},
url = {http://www.ncbi.nlm.nih.gov/pubmed/26048598},
volume = {31},
year = {2015}
