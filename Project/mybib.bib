journal = {European journal of cancer (Oxford, England : 1990)},
keywords = {Antineoplastic Agents,Antineoplastic Agents: therapeutic use,Breast Neoplasms,Breast Neoplasms: pathology,Carcinoma,Carcinoma, Ductal, Breast,Carcinoma, Ductal, Breast: pathology,Carcinoma: pathology,Carcinoma: prevention {\&} control,Disease Progression,Early Diagnosis,Eflornithine,Eflornithine: therapeutic use,Humans,Karyometry,Karyometry: methods,Karyometry: standards,Male,Neoplasms,Neoplasms: pathology,Neoplasms: prevention {\&} control,Precancerous Conditions,Precancerous Conditions: pathology,Precancerous Conditions: prevention {\&} control,Prostatic Intraepithelial Neoplasia,Prostatic Intraepithelial Neoplasia: pathology,Prostatic Neoplasms,Prostatic Neoplasms: pathology,Sensitivity and Specificity},
month = {sep},
