mendeley-tags = {EPI},
month = {jul},
number = {4},
pages = {181--91},
pmid = {19717097},
title = {{U.S. airport entry screening in response to pandemic influenza: modeling and analysis.}},
url = {http://www.ncbi.nlm.nih.gov/pubmed/19717097},
volume = {7},
year = {2009}
}
