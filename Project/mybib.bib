isbn = {1460-2059},
issn = {13674803},
journal = {Bioinformatics},
number = {10},
pages = {1275--1282},
pmid = {23589650},
title = {{Shrinkage estimation of dispersion in Negative Binomial models for RNA-seq experiments with small sample size}},
volume = {29},
