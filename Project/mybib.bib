issn = {0022-104X},
journal = {The Journal of experimental zoology},
keywords = {Adrenal Cortex,Adrenal Cortex: physiology,Animals,Arizona,Birds,Birds: physiology,Blood Proteins,Blood Proteins: metabolism,Corticosterone,Corticosterone: blood,Desert Climate,Female,Handling (Psychology),Male,Seasons,Stress, Physiological,Temperature},
month = {dec},
