keywords = {Aged,Aged, 80 and over,Arthroplasty, Replacement, Knee,Arthroplasty, Replacement, Knee: rehabilitation,Female,Humans,Male,Middle Aged,Osteoarthritis, Knee,Osteoarthritis, Knee: physiopathology,Osteoarthritis, Knee: surgery,Postural Balance,Postural Balance: physiology,Prospective Studies,Quality of Life,Recovery of Function,Treatment Outcome,Weight-Bearing,Weight-Bearing: physiology},
