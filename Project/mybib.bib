journal = {Proceedings of the National Academy of Sciences of the United States of America},
keywords = {Animals,Ecology,Feeding Behavior,Geography,Invertebrates,Lizards,Models, Biological,Population Density,Predatory Behavior,Scorpions,Seawater,Spiders,Spiders: physiology,Vertebrates},
month = {may},
